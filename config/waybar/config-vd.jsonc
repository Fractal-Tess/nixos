      "custom/spacer",
      "cava",
      "custom/spacer",
      "disk#root",
      "custom/spacer",
      "disk#vault",
      "custom/spacer",
      "disk#backup",
      "custom/spacer",
      "network",
      "custom/spacer",
      "cpu",
      "custom/gpu#nvidia",
      "memory",
      "custom/spacer",
