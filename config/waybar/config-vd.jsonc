[
  {
    "layer": "top",
    "position": "top",
    "mod": "dock",
    "output": ["HDMI-A-1"],
    "margin-left": 8,
    "margin-right": 8,
    "margin-top": 8,
    "margin-bottom": 0,
    "height": 32,
    "exclusive": true,
    "passthrough": false,
    "gtk-layer-shell": true,
    "reload_style_on_change": true,
    "modules-left": [
      "custom/padd",
      "custom/nixos",
      "custom/spacer",
      "hyprland/workspaces",
      "custom/padd"
    ],
    "modules-center": [],
    "modules-right": [
